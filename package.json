{
  "name": "silverstripe-nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn generate && cross-env ./bin/run buildManifest",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "prebuild": "yarn generate && cross-env ./bin/run buildManifest",
    "build": "next build",
    "start": "next start",
    "clean": "npx rimraf .next",
    "generate": "cross-env graphql-codegen --config codegen.yml",
    "scaffold-blocks": "cross-env ./bin/run scaffoldBlocks",
    "scaffold-pages": "cross-env ./bin/run scaffoldPages",
    "setup": "cross-env ./bin/run setup"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@babel/parser": "^7.14.5",
    "@babel/traverse": "^7.14.5",
    "@babel/types": "^7.14.5",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.4",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "bootstrap": "^4.6.0",
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "file-system-cache": "^1.0.5",
    "gql-compress": "^2.0.24",
